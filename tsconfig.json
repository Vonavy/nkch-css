{
  "compilerOptions": {
    "target": "es2022",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "module": "commonjs",
    "rootDir": "./src",
    "typeRoots": [
      "./node_modules/@types",
      "./@types",
      "./node_modules/types-mediawiki"
    ],
    "checkJs": true,
    "outDir": "./dist",
    "removeComments": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}